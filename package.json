{
  "name": "@opuscapita/react-reference-select",
  "version": "1.0.0",
  "description": "React reference select",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf coverage lib",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "test": "jest --no-cache --colors",
    "start": "nodemon --watch src/server src/server/demo/index --exec babel-node --presets es2015,stage-0,react --plugins transform-object-assign,transform-decorators-legacy,lodash",
    "npm-build": "rimraf lib && babel --copy-files --no-babelrc --presets es2015,stage-0,react --plugins transform-object-assign,transform-decorators-legacy,lodash --ignore __tests__,*.spec.js,demo src/client/components --out-dir lib",
    "npm-publish": "npm run npm-build && npm-publish",
    "publish-snapshot": "npm run npm-publish",
    "publish-release": "npm run npm-publish -- --release"
  },
  "repository": "OpusCapita/react-reference-select",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "Apache-2.0",
  "readme": "README.md",
  "peerDependencies": {
    "react": "^0.14.8 || ^15.4.2",
    "react-dom": "^0.14.8 || ^15.4.2"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "lodash": "4.17.2",
    "@opuscapita/i18n": "1.0.14",
    "react-bootstrap": "0.30.7",
    "react-select": "1.0.0-rc.2",
    "react-input-autosize": "1.1.0",
    "react-split-pane": "0.1.42",
    "recompose": "0.21.0",
    "superagent": "3.3.0",
    "superagent-bluebird-promise": "3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-plugin-webpack-loaders": "0.8.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "body-parser": "1.15.2",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-config-opuscapita": "1.0.3",
    "eslint-plugin-react": "6.8.0",
    "express": "4.14.0",
    "file-loader": "0.9.0",
    "jest-cli": "18.0.0",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "nodemon": "1.11.0",
    "@opuscapita/npm-scripts": "1.0.8",
    "@opuscapita/react-i18n": "1.0.8",
    "@opuscapita/react-showroom-client": "1.2.10",
    "@opuscapita/react-showroom-server": "1.2.0",
    "postcss-loader": "0.13.0",
    "progress-bar-webpack-plugin": "1.9.0",
    "raw-loader": "0.5.1",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-polyfills-plugin": "0.0.9"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/config/jest-transform-setup.js"
    },
    "moduleNameMapper": {
      "^.*\\.css$": "<rootDir>/config/stubs/CSSStub.js",
      "^.*\\.less$": "<rootDir>/config/stubs/LESSStub.js"
    },
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "lodash",
      "enzyme"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/client/**/*.js",
      "!src/client/**/index.js",
      "!src/client/**/*-test.js",
      "!src/client/**/{en,de,ru}.js"
    ],
    "coverageDirectory": "coverage",
    "verbose": true
  }
}
