{
  "name": "@opuscapita/react-reference-select",
  "version": "2.1.4",
  "description": "React reference select",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf coverage lib",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "test": "jest --no-cache --colors",
    "build-babel": "rimraf lib && babel --copy-files --no-babelrc --presets web-app,react --ignore __tests__,*.spec.js,less,css src --out-dir lib && showroom-scan lib",
    "build-webpack": "rimraf webpack && webpack --entry ./lib/client/demo/index.js --output-path ./lib/client/demo --output-filename bundle.js --mode development --devtool false",
    "start-dev": "npm run build-babel && npm run build-webpack && node lib/server/demo/index.js",
    "start": "nodemon --watch src --exec \"npm run start-dev\"",
    "npm-build": "rimraf lib && babel --copy-files --no-babelrc --presets web-app,react --ignore __tests__,*.spec.js,less,css src/client/components --out-dir lib",
    "npm-publish": "npm run npm-build && npm publish",
    "publish-release": "npm run npm-publish",
    "gh-pages-generate": "npm run build-babel && npm run build-webpack && sh scripts/gh-pages/generate.sh",
    "gh-pages-deploy": "npm run gh-pages-generate && sh scripts/gh-pages/deploy.sh .gh-pages-tmp",
    "test-gh-pages": "npm run gh-pages-generate && http-server .gh-pages-tmp"
  },
  "repository": "OpusCapita/react-reference-select",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "Apache-2.0",
  "readme": "README.md",
  "peerDependencies": {
    "react": "^15.6.2 || ^16.2.0",
    "react-dom": "^15.6.2 || ^16.2.0"
  },
  "dependencies": {
    "@opuscapita/i18n": "1.1.0",
    "babel-preset-web-app": "file:./babel-preset-web-app",
    "bluebird": "3.4.6",
    "lodash": "4.17.2",
    "prop-types": "15.6.0",
    "react-bootstrap": "0.31.5",
    "react-select": "1.1.0",
    "recompose": "0.21.0",
    "superagent": "3.3.0",
    "superagent-bluebird-promise": "3.0.2"
  },
  "devDependencies": {
    "@opuscapita/npm-scripts": "1.0.8",
    "@opuscapita/react-i18n": "2.0.0",
    "@opuscapita/react-showroom-client": "1.3.0-beta.6",
    "@opuscapita/react-showroom-server": "1.3.1",
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-plugin-inline-import": "2.0.6",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-import-styles": "0.0.11",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "body-parser": "1.15.2",
    "chai": "4.1.2",
    "chai-enzyme": "1.0.0-beta.0",
    "css-loader": "0.26.1",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-15": "1.0.5",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "3.12.2",
    "eslint-config-opuscapita": "1.0.3",
    "eslint-plugin-react": "6.8.0",
    "express": "4.14.0",
    "http-server": "0.11.1",
    "jest-cli": "18.0.0",
    "jest-junit-reporter": "1.1.0",
    "nodemon": "1.11.0",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "rimraf": "2.5.4",
    "semver": "5.5.0",
    "webpack": "4.8.0",
    "webpack-cli": "2.1.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/test/mocha-setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/config/jest-transform-setup.js"
    },
    "moduleNameMapper": {
      "^.*\\.css$": "<rootDir>/config/stubs/CSSStub.js",
      "^.*\\.less$": "<rootDir>/config/stubs/LESSStub.js"
    },
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "lodash",
      "enzyme"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/client/**/*.js",
      "!src/client/**/index.js",
      "!src/client/**/*-test.js",
      "!src/client/**/{en,de,ru}.js"
    ],
    "coverageDirectory": "coverage",
    "verbose": true
  }
}
